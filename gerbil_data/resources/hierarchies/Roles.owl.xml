<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
    <!ENTITY dul 'http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#'>
    <!ENTITY iol 'http://www.ontologydesignpatterns.org/ont/dul/IOLite.owl#'>
    <!ENTITY owl 'http://www.w3.org/2002/07/owl#'>
    <!ENTITY owl2xml 'http://www.w3.org/2006/12/owl2-xml#'>
    <!ENTITY rdf 'http://www.w3.org/1999/02/22-rdf-syntax-ns#'>
    <!ENTITY rdfs 'http://www.w3.org/2000/01/rdf-schema#'>
    <!ENTITY xsd 'http://www.w3.org/2001/XMLSchema#'>
]>

<rdf:RDF
    xml:base="http://www.ontologydesignpatterns.org/ont/dul/Roles.owl"
    xmlns="http://www.ontologydesignpatterns.org/ont/dul/Roles.owl#"
    xmlns:Roles="http://www.ontologydesignpatterns.org/ont/dul/Roles.owl#"
    xmlns:dul="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#"
    xmlns:iol="http://www.ontologydesignpatterns.org/ont/dul/IOLite.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

<owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl"/>
    <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/IOLite.owl"/>
    <rdfs:comment rdf:datatype="&xsd;string">An ontology of role types.</rdfs:comment>
    <owl:versionInfo rdf:datatype="&xsd;string"><![CDATA[0.1
Very preliminary as of now. 
As of version 0.1, only some kinds of roles have been input, for use in OntoWordNet alignment.
Author: Aldo Gangemi]]></owl:versionInfo>
</owl:Ontology>

<owl:Class rdf:about="#CausalRole">
    <rdfs:subClassOf rdf:resource="&dul;Role"/>
</owl:Class>

<owl:Class rdf:about="#GeographicalRole">
    <rdfs:subClassOf rdf:resource="&dul;Role"/>
</owl:Class>

<owl:Class rdf:about="#ConceptualRole">
    <rdfs:subClassOf rdf:resource="&dul;Role"/>
</owl:Class>

<owl:ObjectProperty rdf:about="#hasRoleRelatedTo">
    <rdf:type rdf:resource="&owl;SymmetricProperty"/>
    <owl:inverseOf rdf:resource="#hasRoleRelatedTo"/>
    <rdfs:domain rdf:resource="&dul;Entity"/>
    <rdfs:range rdf:resource="&dul;Entity"/>
</owl:ObjectProperty>

<owl:Class rdf:about="#CommercialRole">
    <rdfs:subClassOf rdf:resource="#SocialFunction"/>
</owl:Class>

<owl:Class rdf:about="#AgentRole">
    <rdfs:subClassOf>
        <owl:Restriction>
            <owl:onProperty rdf:resource="&dul;classifies"/>
            <owl:allValuesFrom rdf:resource="&dul;Agent"/>
        </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="&dul;Role"/>
</owl:Class>

<owl:Class rdf:about="#QualitativeRole">
    <rdfs:subClassOf rdf:resource="&dul;Role"/>
</owl:Class>

<owl:Class rdf:about="#SocialFunction">
    <rdfs:subClassOf rdf:resource="&dul;Role"/>
</owl:Class>

<owl:Class rdf:about="#ElementaryRole">
    <rdfs:subClassOf rdf:resource="&dul;Role"/>
</owl:Class>

<owl:Class rdf:about="#Status">
    <rdfs:subClassOf rdf:resource="#SocialFunction"/>
</owl:Class>

<owl:Class rdf:about="#SymmetricRole">
    <rdfs:subClassOf rdf:resource="&dul;Role"/>
</owl:Class>

<owl:Class rdf:about="#FunctionalArea">
    <rdfs:subClassOf>
        <owl:Restriction>
            <owl:onProperty rdf:resource="&dul;isPartOf"/>
            <owl:someValuesFrom rdf:resource="&dul;Organization"/>
        </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="&dul;Role"/>
</owl:Class>

<owl:Class rdf:about="#CollectionRole">
    <rdfs:subClassOf rdf:resource="&dul;Role"/>
</owl:Class>

<owl:Class rdf:about="#Department">
    <rdfs:subClassOf rdf:resource="#FunctionalArea"/>
</owl:Class>

<owl:Class rdf:about="#LegalPossessionRole">
    <rdfs:subClassOf rdf:resource="#SocialFunction"/>
</owl:Class>

<owl:Class rdf:about="#InterpersonalRole">
    <rdfs:subClassOf rdf:resource="&dul;Role"/>
</owl:Class>

<owl:Class rdf:about="#SemioticRole">
    <rdfs:subClassOf rdf:resource="&dul;Role"/>
</owl:Class>

<owl:Class rdf:about="#SubstanceRole">
    <rdfs:subClassOf rdf:resource="&dul;Role"/>
</owl:Class>

<owl:Class rdf:about="#LocativeRole">
    <rdfs:subClassOf rdf:resource="&dul;Role"/>
</owl:Class>

<owl:Class rdf:about="#AgentRole">
    <rdfs:comment rdf:datatype="&xsd;string">A role played only by agents.</rdfs:comment>
    <rdfs:label xml:lang="en">Agent role</rdfs:label>
    <rdfs:label xml:lang="it">Ruolo agentivo</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#CausalRole">
    <rdfs:label xml:lang="en">Causal role</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#CollectionRole">
    <rdfs:label xml:lang="en">Collection role</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#CommercialRole">
    <rdfs:label xml:lang="en">Commercial role</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#ConceptualRole">
    <rdfs:label xml:lang="en">Conceptual role</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#Department">
    <rdfs:comment rdf:datatype="&xsd;string">A main functional area within an Organization</rdfs:comment>
    <rdfs:label xml:lang="it">Dipartimento</rdfs:label>
    <rdfs:label xml:lang="en">Department</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#ElementaryRole">
    <rdfs:comment rdf:datatype="&xsd;string">A role that has no other roles as parts.</rdfs:comment>
    <rdfs:label xml:lang="en">Elementary role</rdfs:label>
    <rdfs:label xml:lang="it">Ruolo individuale</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#FunctionalArea">
    <rdfs:comment rdf:datatype="&xsd;string">A system of roles (e.g. a department) declared by an Organization, which needs Agent s in order to act.</rdfs:comment>
    <rdfs:label xml:lang="en">Functional area</rdfs:label>
    <rdfs:label xml:lang="it">Area funzionale</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#InterpersonalRole">
    <rdfs:comment rdf:datatype="&xsd;string">Un ruolo che caratterizza un agente nell'interazione con altri agenti.</rdfs:comment>
    <rdfs:label xml:lang="en">Interpersonal role</rdfs:label>
    <rdfs:label xml:lang="it">Ruolo interpersonale</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#LegalPossessionRole">
    <rdfs:label xml:lang="en">Legal possession role</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#LocativeRole">
    <rdfs:label xml:lang="en">Locative role</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#QualitativeRole">
    <rdfs:label xml:lang="en">Qualitative role</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#SemioticRole">
    <rdfs:label xml:lang="en">Semiotic role</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#SocialFunction">
    <rdfs:label xml:lang="en">Social function</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#Status">
    <rdfs:label xml:lang="en">Status</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#SubstanceRole">
    <rdfs:label xml:lang="en">Substance role</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#SymmetricRole">
    <rdfs:label xml:lang="en">Symmetric role</rdfs:label>
</owl:Class>

<owl:ObjectProperty rdf:about="#hasRoleRelatedTo">
    <rdfs:comment rdf:datatype="&xsd;string">A relation between entities that are in a same Situation, with different but related Role(s). For example, a dul:Person hasRoleRelatedTo iol:Document when there is a Situation in which both a person and a document have roles with some dependency, e.g. when the person is interested in a topic of that document, when the person acts in order to produce that document, etc.</rdfs:comment>
    <rdfs:label xml:lang="en">has role related to</rdfs:label>
</owl:ObjectProperty>



<owl:Class rdf:about="&dul;Agent"/>

<owl:Class rdf:about="&dul;Entity"/>

<owl:Class rdf:about="&dul;Organization"/>

<owl:Class rdf:about="&dul;Role"/>

<owl:ObjectProperty rdf:about="&dul;classifies"/>

<owl:ObjectProperty rdf:about="&dul;isPartOf"/>

</rdf:RDF>
